package claseTarea4;

class Promedio
{
	double pta;

	double emc;

	double efc;

	//double prom;

	void establecerPTA(double a)
	{
		pta = a;
	}
	double obtenerPTA()
	{
		return pta;
	}
	void establecerEMC(double b)
	{
		emc = b;
	}
	double obtenerEMC()
	{
		return emc;
	}
	void estabalecerEFC(double c)
	{
		efc = c;
	}
	double obtenerEFC()
	{
		return efc;
	}
	public double promedioFinal()
	{
		double prom;

		prom = (pta + emc + efc)/3

		return prom;
	}
}

class Main
{
	public static void main(String[] args)
{
	Promedio prom = new Promedio();

	prom.establecerPTA(12);
	prom.establecerEMC(11);
	prom.establecerEFC(12);

	System.out.printn("El promedio de tareas es: "+prom.establecerPTA());
	System.out.printn("La nota del medio curso es: "+prom.establecerEMC());
	System.out.printn("La nota del fin de curso es: "+prom.establecerEFC());
	System.out.printn("El promedio general es: "+prom.promedioFinal());

}
}
